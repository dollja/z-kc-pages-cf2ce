<!DOCTYPE html>
<html lang="en-US">
	<head>
		
		{% include 'partials/meta_tags.njk' %}

		<title>{{page_title_prefix}}{{page_title}}</title>

		{% include 'partials/ddo.njk' %}

		{% block common_file_includes %}
		{% endblock %}

		{% for filename in additional_js_files %}
			<script src="https://1.www.s81c.com/common/v18/js/{{filename}}.js"></script>
		{% endfor %}

		{% for filename in additional_css_files %}
			<link href="https://1.www.s81c.com/common/v18/css/{{filename}}.css" rel="stylesheet">
		{% endfor %}

		{% block custom_file_includes %}
		{% endblock %}

	</head>

	<body id="ibm-com" class="ibm-type">
		<div id="ibm-top" class="ibm-landing-page">

			{% include 'partials/masthead.njk' %}

			<div id="ibm-content-wrapper">
				<main role="main" aria-labelledby="ibm-pagetitle-h1">
					<div id="ibm-pcon">
						{% block page_content %} {% endblock %}
					</div>
				</main>
			</div>

			{% include 'partials/footer.njk' %}

		</div>
	</body>
</html>