    <body>
        <dds-dotcom-shell-container footer-size="micro" has-profile has-search platform="IBM Z and LinuxONE Content Solutions">
            <main>
                {% include 'partials/dc-leadspace.njk' %}
                <dds-table-of-contents toc-layout="horizontal">
                    <div class="bx--tableofcontents-horizontal__contents">
                        <!-- introduction -->
                        <div class="bx--grid">
                            <div class="bx--row">
                                <a name="introduction" data-title="{{ title }}" class="anchor"></a>
                                <dds-content-item-horizontal-media align="right">
                                    {% if (introMedia.type == "video") %}
                                        <!-- <dds-content-item-horizontal-media-video video-id="1_9h94wo6b" slot="media"></dds-content-item-horizontal-media-video> -->
                                        <dds-video-player-container slot="media" video-id="{{ introMedia.videoID }}" thumbnail-url="{{ introMedia.thumbnailUrl }}"></dds-video-player-container>
                                    {% else %}
                                        <dds-image slot="media" alt="{{ introMedia.label }}" default-src="{{ introMedia.thumbnailUrl }}"></dds-image>
                                    {% endif %}
                                    <dds-content-item-heading>Introduction</dds-content-item-heading>
                                    <dds-content-item-horizontal-copy>
                                        {% from "partials/tooltip.njk" import definitionTooltip %}
                                        {% call definitionTooltip(introParagraphs, definitions) %}
                                        {% endcall %}
                                        {% if (introMedia.noTranscript != true) %}
                                            <a style="font-size:0.875rem;" href="https://izswebpage.mybluemix.net/docs/transcripts/{{ title | replace('/', '-') | replace(' ', '-') }}-Overview-Transcript.pdf" target="_blank" alt="Open overview video transcript in pdf format">Video Transcript</a>
                                        {% endif %}
                                    </dds-content-item-horizontal-copy>
                                    {% if introLinks %}
                                        <dds-link-list slot="footer" type="end">
                                            {% for introLink in introLinks %}
                                                {% if (introLink.type == "marketing") %}
                                                    <dds-link-list-item href="{{ introLink.url }}" type="end" icon-placement="right"> 
                                            See what {{ title }} can do for your business.
                                            <svg focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20" slot="icon">
                                                            <path d="M11.8 2.8L10.8 3.8 16.2 9.3 1 9.3 1 10.7 16.2 10.7 10.8 16.2 11.8 17.2 19 10z"></path>
                                                        </svg>
                                                    </dds-link-list-item>
                                                {% else %}
                                                    <dds-link-list-item href="{{ introLink.url }}" type="end" icon-placement="right">
                                                        {{ introLink.text }}
                                                        <svg focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20" slot="icon">
                                                            <path d="M11.8 2.8L10.8 3.8 16.2 9.3 1 9.3 1 10.7 16.2 10.7 10.8 16.2 11.8 17.2 19 10z"></path>
                                                        </svg>
                                                    </dds-link-list-item>
                                                {% endif %}
                                            {% endfor %}
                                        </dds-link-list>
                                    {% endif %}
                                </dds-content-item-horizontal-media>
                            </div>
                            <!-- bx-row -->

                            <!-- END introduction -->

                            <!-- START VALUE 
                            {% if bannerContent %}
                                <div class="bx--row">
                                    <dds-content-group-banner>
                                        <dds-content-group-heading role="heading" aria-level="3" slot="heading">{{ bannerContent.bannerText }}</dds-content-group-heading>
                                        <dds-link-list type="vertical" slot="complementary">
                                            <dds-link-list-item icon-placement="right" href="{{ bannerContent.bannerLinkUrl }}" cta-type="external">
                                                {{ bannerContent.bannerLinkText }}
                                                <svg focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20" slot="icon">
                                                    <path d="M11.8 2.8L10.8 3.8 16.2 9.3 1 9.3 1 10.7 16.2 10.7 10.8 16.2 11.8 17.2 19 10z"></path>
                                                </svg>
                                            </dds-link-list-item>
                                        </dds-link-list>
                                    </dds-content-group-banner>
                                </div>
                                 bx-row -->
                            <!-- {% endif %}
                            <!- END VALUE -->

                            <!-- Big Picture -->
                            {% if bigPictureSteps %}
                                <div class="bx--row">
                                    <a name="get-started" data-title="How to get started"></a>
                                    <h2 class="bx--layout--top-layout-03">How to get started with {{ title }}</h2>
                                        <dds-card-group class="bx--layout--bottom-layout-03 big-picture-cards">
                                            {% for bpStep in bigPictureSteps %}
                                                <dds-card-group-item pictogram-placement="bottom" class="dds-theme-zone-g10">
                                                    <dds-card-eyebrow>Step</dds-card-eyebrow>
                                                    <dds-card-heading slot="heading" aria-level="4">{{ bpStep.description | safe }}</dds-card-heading>
                                                    {% if bpStep.iconPath %}
                                                        <svg slot="pictogram" focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="64" height="64" viewBox="0 0 32 32">
                                                            {{ bpStep.iconPath | safe }}
                                                        </svg>
                                                    {% else %}
                                                        <object type="image/svg+xml" data="{{ bpStep.iconUrl }}" class="bx--pictogram-item__pictogram" slot="pictogram" style="width:96px; height:96px; padding:0.5rem 0;"></object>
                                                    {% endif %}
                                                </dds-card-group-item>
                                            {% endfor %}
                                        </dds-card-group>
                                </div>
                                <!-- bx-row -->
                            {% endif %}
                            <!-- END Big Picture -->

                            <!-- Getting started tabs -->
                            <div class="bx--row">
                                <dds-tabs-extended orientation="horizontal">
                                    {% for getStartedTab in getStartedTabs %}
                                        <dds-tab label="{{ getStartedTab.tabTitle }}">
                                            {% for tabSubSection in getStartedTab.tabSubSections %}
                                                <section>
                                                    <div class="bx--row">
                                                        <div class="bx--col-lg-{% if tabSubSection.subSectionImage or tabSubSection.subSectionVideo %}10{% else %}16{% endif %}">
                                                            {% if tabSubSection.subSectionTitle %}
                                                                <dds-content-item-heading role="heading" aria-level="3">{{ tabSubSection.subSectionTitle }}</dds-content-item-heading>
                                                            {% endif %}
                                                            {% if tabSubSection.subSectionContent %}
                                                                <dds-content-item-copy>{% call definitionTooltip(tabSubSection.subSectionContent, definitions) %}{% endcall %}</dds-content-item-copy>
                                                            {% endif %}
                                                            {% if tabSubSection.subSectionLinks %}
                                                                <dds-link-list type="vertical" slot="footer">
                                                                    {% for subSectionLink in tabSubSection.subSectionLinks %}
                                                                        <dds-link-list-item icon-placement="right" href="{{ subSectionLink.linkUrl }}" cta-type="{% if subSectionLink.linkTarget == 'newTab' %}external{% else %}local{% endif %}">
                                                                            {{ subSectionLink.linkText }}
                                                                            <svg focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20" slot="icon">
                                                                                <path d="M11.8 2.8L10.8 3.8 16.2 9.3 1 9.3 1 10.7 16.2 10.7 10.8 16.2 11.8 17.2 19 10z"></path>
                                                                            </svg>
                                                                        </dds-link-list-item>
                                                                    {% endfor %}
                                                                </dds-link-list>
                                                            {% endif %}
                                                        </div>
                                                        <!-- layout-x-3 -->
                                                        {% if tabSubSection.subSectionImage %}
                                                            <div class="bx--col-lg-6 bx--col-sm-4">
                                                                <img src="{{ tabSubSection.subSectionImage }}" alt="{{ tabSubSection.subSectionMediaAlt }}" style="max-height:400px; max-width:100%;"/>
                                                                {% if tabSubSection.subSectionCaption %}
                                                                    <p class="ibm-caption ibm-small">{{ tabSubSection.subSectionCaption }}</p>
                                                                {% endif %}
                                                            </div>
                                                            <!-- layout-1-3 -->
                                                        {% endif %}
                                                        {% if tabSubSection.subSectionVideo %}
                                                            <div class="bx--col-lg-6 bx--col-sm-4">
                                                                <dds-video-player-container video-id="{{ tabSubSection.subSectionVideo }}"></dds-video-player-container>
                                                            </div>
                                                            <!-- layout-1-3 -->
                                                        {% endif %}
                                                    </div>
                                                </section>
                                                <!-- bx-row -->
                                            {% endfor %}
                                        </dds-tab>
                                    {% endfor %}
                                </dds-tabs-extended>
                            </div>
                            <!-- END Get started -->

                            <!-- START FAQ  -->
                            {% if faqs %}
                                <div class="bx--row">
                                    <a name="faq" data-title="FAQs"></a>
                                    <dds-content-group-simple>
                                        <dds-content-group-heading>Frequently Asked Questions</dds-content-group-heading>
                                        {% for faq in faqs %}
                                            <dds-content-item>
                                                <dds-content-item-heading role="heading" aria-level="4" slot="heading">
                                                Q: {{ faq.question | safe }}
                                                </dds-content-item-heading>
                                                <dds-content-item-copy>
                                                    <p>A: {% call definitionTooltip(faq.answer, definitions) %}{% endcall %}</p>
                                                </dds-content-item-copy>
                                            </dds-content-item>
                                            {% if faq.linkUrl %}
                                                <dds-text-cta slot="end" icon-placement="right" cta-type="local" href="{{ faq.linkUrl }}">
                                                    {{ faq.linkText }}
                                                </dds-text-cta>
                                            {% endif %}
                                        {% endfor %}
                                        {% if learnMore %}
                                            <dds-content-item slot="complementary" class="bx--layout--top-layout-03">
                                                <dds-content-item-heading role="heading" aria-level="4">Learn more</dds-content-item-heading>
                                                {{ learnMore | safe }}
                                            </dds-content-item>
                                        {% endif %}
                                    </dds-content-group-simple>
                                </div>
                            {% endif %}
                            {# For multi-section FAQ, will refactor to combine these later  #}
                            {% if faqSection %}
                                <a name="faq" data-title="FAQs"></a>
                                <dds-content-group-simple>
                                    <dds-content-group-heading>Frequently Asked Questions</dds-content-group-heading>
                                    {% for aSection in faqSection %}
                                        <dds-content-item>
                                            <dds-content-item-heading role="heading" aria-level="3" slot="heading" class="bx--layout--bottom-layout-02">
                                                {{ aSection.section }}
                                            </dds-content-item-heading>
                                            <bx-accordion class="bx--layout--top-layout-03 bx--layout--bottom-layout-04">
                                                {% for category in aSection.categories %}
                                                    <bx-accordion-item title-text="{{ category.catTitle }}">
                                                        <div>
                                                            {% for faq in category.faqs %}
                                                                <h5 class="bx--layout--top-layout-03">
                                                                    <strong>Q: {{ faq.question | safe }}</strong>
                                                                </h5>
                                                                <p>A: {% call definitionTooltip(faq.answer, definitions) %}{% endcall %}</p>
                                                                {% if faq.linkUrl %}
                                                                    <dds-text-cta slot="end" icon-placement="right" cta-type="local" href="{{ faq.linkUrl }}">
                                                                        {{ faq.linkText }}
                                                                    </dds-text-cta>
                                                                {% endif %}
                                                            {% endfor %}
                                                        </div>
                                                    </bx-accordion-item>
                                                {% endfor %}
                                            </bx-accordion>
                                        </dds-content-item>
                                    {% endfor %}
                                </dds-content-group-simple>
                            {% endif %}

                            <!-- END FAQ -->
                            <a name="documentation" data-title="Documentation"></a>
                            <div class="bx--row">
                                <dds-card-in-card href="https://example.com" class="dds-theme-zone-g90 bx--layout--top-layout-03 bx--layout--bottom-layout-06">
                                    <dds-card-in-card-image slot="image" alt="Documentation illustration" default-src="https://trello.com/1/cards/5dcd8edc161e3a6c1cbedb90/attachments/5dcf0fca4f1fd667f61cc962/previews/5dcf0fcb4f1fd667f61cca32/download/Asset_3%403xbluetransparent.png"></dds-card-in-card-image>
                                    <dds-card-eyebrow>Documentation</dds-card-eyebrow>
                                    <dds-card-heading>Comprehensive content collection</dds-card-heading>
                                    <p>Description of C3</p>
                                    <dds-card-cta-footer>
                                        <svg slot="icon" focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20" >
                                            <path d="M11.8 2.8L10.8 3.8 16.2 9.3 1 9.3 1 10.7 16.2 10.7 10.8 16.2 11.8 17.2 19 10z"></path>
                                        </svg>
                                    </dds-card-cta-footer>
                                </dds-card-in-card>
                            </div>
                            <!-- START RESOURCES SECTION -->
                            <div class="bx--row">
                                <a name="technical-resources" data-title="Technical resources"></a>
                                <h2>Technical resources for {{ title }}</h2>
                                <dds-tabs-extended orientation="horizontal" class="bx--layout--top-layout-03 bx--layout--bottom-layout-06">
                                    {% for resourcesTab in resourcesTabs %}
                                        <dds-tab label="{{ resourcesTab.tabTitle }}">
                                            <dds-card-group>
                                                {% for tabContent in resourcesTab.tabContent %}
                                                    {% if tabContent.resourceStatus != "hide" %}
                                                        <dds-card-group-item href="{{ tabContent.resourceLinkUrl }}">
                                                            <dds-card-eyebrow>
                                                            Resource type
                                                            </dds-card-eyebrow>
                                                            {% if tabContent.resourceTitle %}
                                                                <dds-card-heading>{{ tabContent.resourceTitle }}</dds-card-heading>
                                                            {% endif %}
                                                            {% if tabContent.resourceDescription %}
                                                                <p>
                                                                    <bx-tag type="gray" title="Clear selection"> Resource type </bx-tag>
                                                                    {% if tabContent.resourceStatus %}
                                                                        <bx-tag type="blue" title="Clear selection"> New </bx-tag>
                                                                    {% endif %}
                                                                </p>
                                                                <p>
                                                                    {{ tabContent.resourceDescription | safe }}
                                                                </p>
                                                            {% endif %}
                                                            <dds-card-cta-footer slot="footer">{{ tabContent.resourceLinkText }}
                                                                <svg slot="icon" focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20">
                                                                    <path d="M11.8 2.8L10.8 3.8 16.2 9.3 1 9.3 1 10.7 16.2 10.7 10.8 16.2 11.8 17.2 19 10z"></path>
                                                                </svg>
                                                            </dds-card-cta-footer>
                                                        </dds-card-group-item>
                                                    {% endif %}
                                                {% endfor %}
                                            </dds-card-group>
                                        </dds-tab>
                                    {% endfor %}
                                </dds-tabs-extended>
                            </div>
                            <!-- END RESOURCES SECTION -->

                            <!-- START RESOURCES SECTION 
                            <div class="bx--row">
                                <a name="technical-resources" data-title="Technical resources"></a>
                                <h2>Technical resources for {{ title }}</h2>
                                <dds-tabs-extended orientation="horizontal" class="bx--layout--top-layout-03 bx--layout--bottom-layout-06">
                                    {% for resourcesTab in resourcesTabs %}
                                        <dds-tab label="{{ resourcesTab.tabTitle }}">
                                            <dds-card-group grid-mode="narrow">
                                                {% for tabContent in resourcesTab.tabContent %}
                                                    {% if tabContent.resourceStatus != "hide" %}
                                                         <dds-card-group-card-link-item href="{{ tabContent.resourceLinkUrl }}" pattern-background>
                                                            {% if tabContent.resourceTitle %}
                                                                <dds-card-link-heading>{{ tabContent.resourceTitle }}</dds-card-link-heading>
                                                            {% endif %}
                                                            {% if tabContent.resourceDescription %}
                                                                <p>
                                                                    {% if tabContent.resourceStatus %}
                                                                        <strong style="color:yellow;">NEW! </strong>
                                                                    {% endif %}
                                                                    {{ tabContent.resourceDescription | safe }}
                                                                </p>
                                                            {% endif %}
                                                            <dds-card-cta-footer slot="footer">{{ tabContent.resourceLinkText }}
                                                                <svg slot="icon" focusable="false" preserveAspectRatio="xMidYMid meet" xmlns="http://www.w3.org/2000/svg" fill="currentColor" aria-hidden="true" width="20" height="20" viewBox="0 0 20 20">
